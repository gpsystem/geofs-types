{
  "name": "@geps/geofs-types",
  "version": "0.3.0-dev",
  "description": "Types for the GeoFS API.",
  "types": "typings/index.d.ts",
  "scripts": {
    "check-format": "prettier . --check --ignore-path .gitignore",
    "check-spelling": "cspell --config=.cspell.json \"**/*.{md,mdx,ts,js,tsx,jsx}\"",
    "prettier": "prettier . --write --ignore-path .gitignore",
    "lint": "eslint . --ignore-path .gitignore",
    "test": "check-dts",
    "precommit": "tsc --noEmit && npm test && npm run check-format && npm run lint && npm run check-spelling",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gpsystem/geofs-types.git"
  },
  "keywords": [
    "geofs",
    "types"
  ],
  "contributors": [
    "Guy Adler (https://github.com/Guy-Adler)",
    "Nicolas Rodriguez (https://github.com/nicolas377)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gpsystem/geofs-types/issues"
  },
  "homepage": "https://github.com/gpsystem/geofs-types#readme",
  "dependencies": {
    "@types/jquery": "3.5.11",
    "@types/leaflet": "1.7.8",
    "cesium": "1.89.0"
  },
  "devDependencies": {
    "@types/node": "~17.0.8",
    "@typescript-eslint/eslint-plugin": "~5.9.0",
    "check-dts": "~0.6.6",
    "cspell": "~5.15.2",
    "eslint": "~8.6.0",
    "eslint-config-prettier": "~8.3.0",
    "husky": "~7.0.4",
    "prettier": "~2.5.1",
    "typescript": "~4.5.4"
  }
}
